{
  "manifest_version": 2,
  "version": "0.0.0",
  "name": "My Way",
  "description": "Extension for Chrome to help people in your daily management!",
  "author": "Jos√© Roberto Alves Junior",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "XFrameAllow",
    "default_popup": "index.html",
    "default_icon": "img/icon_128.png"
  },
  "icons":{
    "16":"img/icon_16.png",
    "48":"img/icon_48.png",
    "128":"img/icon_128.png"
  },
  "options_page":"page/options/options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*/*"
  ]
}
